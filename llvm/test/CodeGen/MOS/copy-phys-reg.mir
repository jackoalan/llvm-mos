# RUN: llc -mtriple=mos -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name:            tax
tracksRegLiveness: true
# CHECK-LABEL: name: tax
body:             |
  bb.0.entry:
    liveins: $a
    $x = COPY $a
# CHECK: $x = TA_
...
---
name:            txa
tracksRegLiveness: true
# CHECK-LABEL: name: txa
body:             |
  bb.0.entry:
    liveins: $x
    $a = COPY $x
# CHECK: T_A $x
...
---
name:            tay
tracksRegLiveness: true
# CHECK-LABEL: name: tay
body:             |
  bb.0.entry:
    liveins: $a
    $y = COPY $a
# CHECK: $y = TA_
...
---
name:            tya
tracksRegLiveness: true
# CHECK-LABEL: name: tya
body:             |
  bb.0.entry:
    liveins: $y
    $a = COPY $y
# CHECK: T_A $y
...
---
name:            txy
tracksRegLiveness: true
# CHECK-LABEL: name: txy
body:             |
  bb.0.entry:
    liveins: $x
    $y = COPY $x
# CHECK:      T_A $x
# CHECK-NEXT: $y = TA_
...
---
name:            tyx
tracksRegLiveness: true
# CHECK-LABEL: name: tyx
body:             |
  bb.0.entry:
    liveins: $y
    $x = COPY $y
# CHECK:      T_A $y
# CHECK-NEXT: $x = TA_
...
---
name:            preserve
tracksRegLiveness: true
# CHECK-LABEL: name: preserve
body:             |
  bb.0.entry:
    liveins: $a, $x
    $y = COPY $x
    CMPimm $a, 0, implicit-def $nz
# CHECK:      STabs $a, &_SaveA
# CHECK-NEXT: T_A $x
# CHECK-NEXT: $y = TA_
# CHECK-NEXT: PHP
# CHECK-NEXT: $a = LDabs &_SaveA
# CHECK-NEXT: PLP
...
---
name:            stzp
tracksRegLiveness: true
# CHECK-LABEL: name: stzp
body:             |
  bb.0.entry:
    liveins: $a
    $zp_0 = COPY $a
# CHECK: $zp_0 = STzpr $a
...
---
name:            ldzp
tracksRegLiveness: true
# CHECK-LABEL: name: ldzp
body:             |
  bb.0.entry:
    liveins: $zp_0
    $a = COPY $zp_0
# CHECK: $a = LDzpr $zp_0
...
---
name:            zptozp_a
tracksRegLiveness: true
# CHECK-LABEL: name: zptozp_a
body:             |
  bb.0.entry:
    liveins: $zp_0
    $zp_1 = COPY $zp_0
# CHECK:      $a = LDzpr $zp_0
# CHECK-NEXT: $zp_1 = STzpr $a
...
---
name:            zptozp_x
tracksRegLiveness: true
# CHECK-LABEL: name: zptozp_x
body:             |
  bb.0.entry:
    liveins: $zp_0, $a
    $zp_1 = COPY $zp_0
    RTS implicit $a
# CHECK:      $x = LDzpr $zp_0
# CHECK-NEXT: $zp_1 = STzpr $x
# CHECK-NEXT: RTS
...
---
name:            zptozp_y
tracksRegLiveness: true
# CHECK-LABEL: name: zptozp_y
body:             |
  bb.0.entry:
    liveins: $zp_0, $a, $x
    $zp_1 = COPY $zp_0
    RTS implicit $a, implicit $x
# CHECK:      $y = LDzpr $zp_0
# CHECK-NEXT: $zp_1 = STzpr $y
# CHECK-NEXT: RTS
...
---
name:            zptozp_save
tracksRegLiveness: true
# CHECK-LABEL: name: zptozp_save
body:             |
  bb.0.entry:
    liveins: $zp_0, $a, $x, $y
    $zp_1 = COPY $zp_0
    RTS implicit $a, implicit $x, implicit $y
# CHECK:      STabs $a, &_SaveA
# CHECK-NEXT: $a = LDzpr $zp_0
# CHECK-NEXT: $zp_1 = STzpr $a
# CHECK-NEXT: PHP
# CHECK-NEXT: $a = LDabs &_SaveA
# CHECK-NEXT: PLP
# CHECK-NEXT: RTS
...
---
name:            zpptr_to_zpptr
tracksRegLiveness: true
# CHECK-LABEL: name: zpptr_to_zpptr
body:             |
  bb.0.entry:
    liveins: $zp_ptr_0
    $zp_ptr_1 = COPY $zp_ptr_0
# CHECK:      $a = LDzpr $zp_0
# CHECK-NEXT: $zp_2 = STzpr $a
# CHECK:      $a = LDzpr $zp_1
# CHECK-NEXT: $zp_3 = STzpr $a
...
