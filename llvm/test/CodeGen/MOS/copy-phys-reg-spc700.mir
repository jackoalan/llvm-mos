# RUN: llc -mtriple=mos -mcpu=mosspc700 -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name: alsb_to_v
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: $alsb
    ; CHECK-LABEL: name: alsb_to_v
    ; CHECK: liveins: $alsb
    ; CHECK: dead $c = CMPImm $a, 0, implicit-def $nz
    ; CHECK: $v = SelectImm $z, 0, -1
    $v = COPY $alsb
...
---
name: xlsb_to_v
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: $xlsb
    ; CHECK-LABEL: name: xlsb_to_v
    ; CHECK: liveins: $xlsb
    ; CHECK: dead $c = CMPImm $x, 0, implicit-def $nz
    ; CHECK: $v = SelectImm $z, 0, -1
    $v = COPY $xlsb
...
---
name: ylsb_to_v
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: $ylsb
    ; CHECK-LABEL: name: ylsb_to_v
    ; CHECK: liveins: $ylsb
    ; CHECK: dead $c = CMPImm $y, 0, implicit-def $nz
    ; CHECK: $v = SelectImm $z, 0, -1
    $v = COPY $ylsb
...
