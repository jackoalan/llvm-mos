# RUN: llc -mtriple=mos6502 -run-pass=prologepilog -verify-machineinstrs -o - %s | FileCheck %s
---
name: stack
# CHECK-LABEL: name: stack
stack:
  - { id: 0, size: 1}
  - { id: 1, size: 1}
  - { id: 2, size: 1}
  - { id: 3, size: 1}
  - { id: 4, size: 1}
body:             |
  bb.0.entry:
    STstk $a, %stack.4 :: (store 1 into %stack.4)
    $a = LDstk %stack.4 :: (load 1 from %stack.4)
    STstk $a, %stack.0 :: (store 1 into %stack.0)
    $a, $c = AddrLostk %stack.4, implicit-def $nz, implicit $sp, implicit $c
    $a = AddrHistk %stack.4, $c, implicit-def $nz, implicit-def $c, implicit $sp
    $zp_ptr_0 = Addrstk %stack.4, implicit-def $nz, implicit $sp
    STstk $a, %stack.2 :: (store 1 into %stack.2)
    STstk $a, %stack.1 :: (store 1 into %stack.1)
    $a, $c = AddrLostk %stack.1, implicit-def $nz, implicit $sp, implicit $c
    $a = AddrHistk %stack.1, $c, implicit-def $nz, implicit-def $c, implicit $sp
    $zp_ptr_0 = Addrstk %stack.1, implicit-def $nz, implicit $sp
    $a = LDstk %stack.1 :: (load 1 from %stack.1)
    $a = LDstk %stack.2 :: (load 1 from %stack.2)
    $a = LDstk %stack.0 :: (load 1 from %stack.0)
    RTS
# CHECK-LABEL: bb.0.entry:
# CHECK-NEXT:  IncSP -1
# CHECK-NEXT:  STindirimm $a, $sp, 0 :: (store 1 into %stack.4)
# CHECK-NEXT:  $a = LDindirimm $sp, 0 :: (load 1 from %stack.4)
# CHECK-NEXT:  Push $a
# CHECK-NEXT:  $a, $c = AddrLoss 0
# CHECK-NEXT:  $a = AddrHiss 0, $c
# CHECK-NEXT:  $zp_ptr_0 = Addrss 0
# CHECK-NEXT:  Push undef $a
# CHECK-NEXT:  Push $a
# CHECK-NEXT:  Push undef $a
# CHECK-NEXT:  SThs $a, 3
# CHECK-NEXT:  $a, $c = AddrLohs 3
# CHECK-NEXT:  $a = AddrHihs 3, $c
# CHECK-NEXT:  $zp_ptr_0 = Addrhs 3
# CHECK-NEXT:  $a = LDhs 3
# CHECK-NEXT:  dead $a = Pull
# CHECK-NEXT:  $a = Pull
# CHECK-NEXT:  dead $a = Pull
# CHECK-NEXT:  $a = Pull
# CHECK-NEXT:  IncSP 1
# CHECK-NEXT:  RTS
...
---
name: ptr
# CHECK-LABEL: name: ptr
stack:
  - { id: 0, size: 2}
  - { id: 1, size: 2}
  - { id: 2, size: 2}
body: |
  bb.0.entry:
    STPtrstk $zp_ptr_0, %stack.2 :: (store 2 into %stack.2)
    $zp_ptr_0 = LDPtrstk %stack.2 :: (load 2 from %stack.2)
    STPtrstk $zp_ptr_0, %stack.0 :: (store 2 into %stack.0)
    $zp_ptr_0 = LDPtrstk %stack.1 :: (load 2 from %stack.1)
    STPtrstk $zp_ptr_0, %stack.1 :: (store 2 into %stack.1)
    $zp_ptr_0 = LDPtrstk %stack.0 :: (load 2 from %stack.0)
    RTS
# CHECK-LABEL: bb.0.entry:
# CHECK-NEXT:  IncSP -2
# CHECK-NEXT:  STindirimm $zp_0, $sp, 0
# CHECK-NEXT:  STindirimm $zp_1, $sp, 1
# CHECK-NEXT:  $zp_0 = LDindirimm $sp, 0
# CHECK-NEXT:  $zp_1 = LDindirimm $sp, 1
# CHECK-NEXT:  Push $zp_1
# CHECK-NEXT:  Push $zp_0
# CHECK-NEXT:  Push undef $a
# CHECK-NEXT:  Push undef $a
# CHECK-NEXT:  $zp_0 = LDhs 1
# CHECK-NEXT:  $zp_1 = LDhs 2
# CHECK-NEXT:  SThs $zp_0, 1
# CHECK-NEXT:  SThs $zp_1, 2
# CHECK-NEXT:  dead $a = Pull
# CHECK-NEXT:  dead $a = Pull
# CHECK-NEXT:  $zp_0 = Pull
# CHECK-NEXT:  $zp_1 = Pull
# CHECK-NEXT:  IncSP 2
# CHECK-NEXT:  RTS
...
---
name: epilogue_no_terminator
# CHECK-LABEL: name: epilogue_no_terminator
frameInfo:
  savePoint: '%bb.0.entry'
  restorePoint: '%bb.0.entry'
stack:
  - { id: 0, size: 1}
tracksRegLiveness: true
body:             |
  bb.0.entry:
    $a = IMPLICIT_DEF

  bb.1:
    RTS
# CHECK-LABEL: bb.0.entry:
# CHECK:       $a = IMPLICIT_DEF
# CHECK-NEXT:  Push undef $a
# CHECK-NEXT:  dead $a = Pull
...
