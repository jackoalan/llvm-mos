# RUN: llc -mtriple=mos6502 -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name: addfilo
# CHECK-LABEL: name: addfilo
body: |
  bb.0.entry:
    $a, $c = AddFiLo 1234, implicit-def $nz, implicit $sp
# CHECK:      $a = LDimm 210
# CHECK-NEXT: $c = LDCimm 0
# CHECK-NEXT: $a, $c = ADCzpr $a, $splo, $c
...
---
name: addfilo0
# CHECK-LABEL: name: addfilo0
body: |
  bb.0.entry:
    $a, $c = AddFiLo 512, implicit-def $nz, implicit $sp
# CHECK: $a = LDzpr $splo
...
---
name: addfilo_copy
# CHECK-LABEL: name: addfilo_copy
body: |
  bb.0.entry:
    $zp_1, $c = AddFiLo 512, implicit-def $nz, implicit $sp
# CHECK:      $a = LDzpr $splo
# CHECK-NEXT: $zp_1 = STzpr $a
...
---
name: adcfihi
# CHECK-LABEL: name: adcfihi
body: |
  bb.0.entry:
    $a = AdcFiHi 1234, $c, implicit-def $c, implicit-def $nz, implicit $sp
# CHECK:      $a = LDimm 4
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
...
---
name: adcfihi_lo0
# CHECK-LABEL: name: adcfihi
body: |
  bb.0.entry:
    $a = AdcFiHi 512, $c, implicit-def $c, implicit-def $nz, implicit $sp
# CHECK:      $a = LDimm 2
# CHECK-NEXT: LDCimm 0
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
...
---
name: addfi
# CHECK-LABEL: name: addfi
body: |
  bb.0.entry:
    $zp_ptr_0 = AddFi 1234, implicit-def $c, implicit-def $nz, implicit $sp
    RTS
# CHECK:      $a = LDimm 210
# CHECK-NEXT: $c = LDCimm 0
# CHECK-NEXT: $a, $c = ADCzpr $a, $splo, $c
# CHECK-NEXT: $zp_0 = STzpr $a
# CHECK-NEXT: $a = LDimm 4
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
# CHECK-NEXT: $zp_1 = STzpr $a
# CHECK-NEXT: RTS
...
---
name: incsp
# CHECK-LABEL: name: incsp
body: |
  bb.0.entry:
    IncSP -1234, implicit-def $sp, implicit $sp
    RTS
# CHECK:      $c = LDCimm 0
# CHECK-NEXT: $a = LDimm 46
# CHECK-NEXT: $a, $c = ADCzpr $a, $splo, $c
# CHECK-NEXT: $splo = STzpr $a
# CHECK-NEXT: $a = LDimm 251
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
# CHECK-NEXT: $sphi = STzpr $a
...
---
name: incsp_hi_only
# CHECK-LABEL: name: incsp
body: |
  bb.0.entry:
    IncSP -512, implicit-def $sp, implicit $sp
    RTS
# CHECK:      $c = LDCimm 0
# CHECK-NEXT: $a = LDimm 254
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
# CHECK-NEXT: $sphi = STzpr $a
...
---
name: ldaidx
# CHECK-LABEL: name: ldaidx
body: |
  bb.0.entry:
    liveins: $x
    $a = LDidx 1234, $x, implicit-def $nz
# CHECK: LDAidx 1234, $x
...
---
name: ldxidx_y
# CHECK-LABEL: name: ldxidx_y
body: |
  bb.0.entry:
    liveins: $y
    $x = LDidx 1234, $y, implicit-def $nz
# CHECK: LDXidx 1234
...
---
name: ldxidx_x
# CHECK-LABEL: name: ldxidx_x
body: |
  bb.0.entry:
    liveins: $x
    $x = LDidx 1234, $x, implicit-def $nz
    CMPimm $a, 0, implicit-def $nz
# CHECK:      STabs $a, &_SaveA
# CHECK-NEXT: LDAidx 1234, $x
# CHECK-NEXT: $x = TA_
# CHECK-NEXT: PHP
# CHECK-NEXT: $a = LDabs &_SaveA
# CHECK-NEXT: PLP
...
---
name: ldyidx_x
# CHECK-LABEL: name: ldyidx_x
body: |
  bb.0.entry:
    liveins: $x
    $y = LDidx 1234, $x, implicit-def $nz
# CHECK: LDYidx 1234
...
---
name: ldhs
# CHECK-LABEL: name: ldhs
body: |
  bb.0.entry:
    $x = LDhs 2
# CHECK:      TSX
# CHECK-NEXT: LDAidx 258, $x
# CHECK-NEXT: $x = TA_
...
---
name: sths
# CHECK-LABEL: name: sths
body: |
  bb.0.entry:
    SThs $a, 2
# CHECK:      TSX
# CHECK-NEXT: STidx $a, 258, $x
...
---
name: pushhs_a
# CHECK-LABEL: name: pushhs_a
body: |
  bb.0.entry:
    liveins: $a
    Pushhs $a
    RTS
# CHECK:      PHA
# CHECK-NEXT: RTS
...
---
name: pushhs_x
# CHECK-LABEL: name: pushhs_x
body: |
  bb.0.entry:
    liveins: $x
    Pushhs $x
    RTS
# CHECK:      T_A $x
# CHECK-NEXT: PHA
# CHECK-NEXT: RTS
...
---
name: pushptrhs
# CHECK-LABEL: name: pushptrhs
body: |
  bb.0.entry:
    liveins: $zp_ptr_0
    PushPtrhs $zp_ptr_0
    RTS
# CHECK:      $a = LDzpr $zp_1
# CHECK-NEXT: PHA
# CHECK-NEXT: $a = LDzpr $zp_0
# CHECK-NEXT: PHA
# CHECK-NEXT: RTS
...
---
name: pullhs_a
# CHECK-LABEL: name: pullhs_a
body: |
  bb.0.entry:
    $a = Pullhs
    RTS implicit $a
# CHECK:      PLA
# CHECK-NEXT: RTS
...
---
name: pullhs_x
# CHECK-LABEL: name: pullhs_x
body: |
  bb.0.entry:
    $x = Pullhs
    RTS implicit $x
# CHECK:      PLA
# CHECK-NEXT: $x = TA_
# CHECK-NEXT: RTS
...
---
name: pullptrhs
# CHECK-LABEL: name: pullptrhs
body: |
  bb.0.entry:
    $zp_ptr_0 = PullPtrhs
    RTS implicit $zp_ptr_0
# CHECK:      PLA
# CHECK-NEXT: $zp_0 = STzpr $a
# CHECK-NEXT: PLA
# CHECK-NEXT: $zp_1 = STzpr $a
# CHECK-NEXT: RTS
...
