# RUN: llc -mtriple=mos6502 -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name: addrloss
# CHECK-LABEL: name: addrloss
body: |
  bb.0.entry:
    $zp_0, $c = AddrLoss 1234, implicit-def $nz
# CHECK:      $a = LDimm 210
# CHECK-NEXT: $c = LDCimm 0
# CHECK-NEXT: $a, $c = ADCzpr $a, $splo, $c
# CHECK-NEXT: $zp_0 = STzpr $a
...
---
name: addrloss0
# CHECK-LABEL: name: addrloss0
body: |
  bb.0.entry:
    $zp_0, $c = AddrLoss 512, implicit-def $nz
# CHECK:      $a = LDzpr $splo
# CHECK-NEXT: $zp_0 = STzpr $a
...
---
name: addrhiss
# CHECK-LABEL: name: addrhiss
body: |
  bb.0.entry:
    $a = AddrHiss 1234, $c, implicit-def $c, implicit-def $nz
# CHECK:      $a = LDimm 4
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
...
---
name: addrhiss_lo0
# CHECK-LABEL: name: addrhiss_lo0
body: |
  bb.0.entry:
    $a = AddrHiss 512, $c, implicit-def $c, implicit-def $nz
# CHECK:      $a = LDimm 2
# CHECK-NEXT: LDCimm 0
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
...
---
name: addrss
# CHECK-LABEL: name: addrss
body: |
  bb.0.entry:
    $zp_ptr_0 = Addrss 1234, implicit-def $c, implicit-def $nz
    RTS
# CHECK:      $a = LDimm 210
# CHECK-NEXT: $c = LDCimm 0
# CHECK-NEXT: $a, $c = ADCzpr $a, $splo, $c
# CHECK-NEXT: $zp_0 = STzpr $a
# CHECK-NEXT: $a = LDimm 4
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
# CHECK-NEXT: $zp_1 = STzpr $a
# CHECK-NEXT: RTS
...
---
name: addrlohs
# CHECK-LABEL: name: addrlohs
body: |
  bb.0.entry:
    $x, $c = AddrLohs 123, implicit-def $nz
# CHECK:      TSX
# CHECK-NEXT: T_A $x
# CHECK-NEXT: $c = LDCimm 0
# CHECK-NEXT: $a, $c = ADCimm $a, 123, $c
# CHECK-NEXT: $x = TA_
...
---
name: addrlohs0
# CHECK-LABEL: name: addrlohs0
body: |
  bb.0.entry:
    $a, $c = AddrLohs 0, implicit-def $nz
# CHECK:      TSX
# CHECK-NEXT: T_A $x
...
---
name: addrhihs
# CHECK-LABEL: name: addrhihs
body: |
  bb.0.entry:
    $x = AddrHihs 123, $c, implicit-def $c, implicit-def $nz
# CHECK:      $a = LDimm 1
# CHECK-NEXT: $a, dead $c = ADCimm $a, 0, $c
# CHECK-NEXT: $x = TA_
...
---
name: addrhihs0
# CHECK-LABEL: name: addrhihs0
body: |
  bb.0.entry:
    $x = AddrHihs 0, $c, implicit-def $c, implicit-def $nz
    RTS
# CHECK:      $x = LDimm 1
# CHECK-NEXT: RTS
...
---
name: addrhihs0_copy
# CHECK-LABEL: name: addrhihs0
body: |
  bb.0.entry:
    $zp_0 = AddrHihs 0, $c, implicit-def $c, implicit-def $nz
    RTS
# CHECK:      $a = LDimm 1
# CHECK-NEXT: $zp_0 = STzpr $a
# CHECK-NEXT: RTS
...
---
name: addrhs
# CHECK-LABEL: name: addrhs
body: |
  bb.0.entry:
    $zp_ptr_0 = Addrhs 123, implicit-def $c, implicit-def $nz
    RTS
# CHECK:      TSX
# CHECK-NEXT: T_A $x
# CHECK-NEXT: $c = LDCimm 0
# CHECK-NEXT: $a, $c = ADCimm $a, 123, $c
# CHECK-NEXT: $zp_0 = STzpr $a
# CHECK-NEXT: $a = LDimm 1
# CHECK-NEXT: $a, dead $c = ADCimm $a, 0, $c
# CHECK-NEXT: $zp_1 = STzpr $a
# CHECK-NEXT: RTS
...
---
name: incsp
# CHECK-LABEL: name: incsp
body: |
  bb.0.entry:
    IncSP -1234
    RTS
# CHECK:      $c = LDCimm 0
# CHECK-NEXT: $a = LDimm 46
# CHECK-NEXT: $a, $c = ADCzpr $a, $splo, $c
# CHECK-NEXT: $splo = STzpr $a
# CHECK-NEXT: $a = LDimm 251
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
# CHECK-NEXT: $sphi = STzpr $a
...
---
name: incsp_hi_only
# CHECK-LABEL: name: incsp
body: |
  bb.0.entry:
    IncSP -512
    RTS
# CHECK:      $c = LDCimm 0 
# CHECK-NEXT: $a = LDimm 254
# CHECK-NEXT: $a, dead $c = ADCzpr $a, $sphi, $c
# CHECK-NEXT: $sphi = STzpr $a
...
---
name: ldindirimm
# CHECK-LABEL: name: ldindirimm
body: |
  bb.0.entry:
    $zp_0 = LDindirimm $zp_ptr_0, 123
    RTS
# CHECK:      $y = LDimm 123
# CHECK-NEXT: $a = LDyindirr $zp_ptr_0, $y
# CHECK-NEXT: $zp_0 = STzpr $a
# CHECK-NEXT: RTS
...
---
name: stindirimm
# CHECK-LABEL: name: stindirimm
body: |
  bb.0.entry:
    STindirimm $zp_0, $zp_ptr_0, 123
    RTS
# CHECK:      $a = LDzpr $zp_0
# CHECK-NEXT: $y = LDimm 123
# CHECK-NEXT: STyindirr $a, $zp_ptr_0, $y
# CHECK-NEXT: RTS
...
---
name: ldaidx
# CHECK-LABEL: name: ldaidx
body: |
  bb.0.entry:
    liveins: $x
    $a = LDidx 1234, $x, implicit-def $nz
# CHECK: LDAidx 1234, $x
...
---
name: ldxidx_y
# CHECK-LABEL: name: ldxidx_y
body: |
  bb.0.entry:
    liveins: $y
    $x = LDidx 1234, $y, implicit-def $nz
# CHECK: LDXidx 1234
...
---
name: ldxidx_x
# CHECK-LABEL: name: ldxidx_x
body: |
  bb.0.entry:
    liveins: $x
    $x = LDidx 1234, $x, implicit-def $nz
    CMPimm $a, 0, implicit-def $nz
# CHECK:      STabs $a, &_SaveA
# CHECK-NEXT: LDAidx 1234, $x
# CHECK-NEXT: $x = TA_
# CHECK-NEXT: $a = LDabs &_SaveA
# CHECK-NEXT: CMPimm $a, 0
...
---
name: ldyidx_x
# CHECK-LABEL: name: ldyidx_x
body: |
  bb.0.entry:
    liveins: $x
    $y = LDidx 1234, $x, implicit-def $nz
# CHECK: LDYidx 1234
...
---
name: ldhs
# CHECK-LABEL: name: ldhs
body: |
  bb.0.entry:
    $x = LDhs 2
# CHECK:      TSX
# CHECK-NEXT: LDAidx 258, $x
# CHECK-NEXT: $x = TA_
...
---
name: ldhs_copy
# CHECK-LABEL: name: ldhs_copy
body: |
  bb.0.entry:
    $zp_0 = LDhs 2
# CHECK:      TSX
# CHECK-NEXT: LDAidx 258, $x
# CHECK-NEXT: $zp_0 = STzpr $a
...
---
name: sths
# CHECK-LABEL: name: sths
body: |
  bb.0.entry:
    SThs $a, 2
# CHECK:      TSX
# CHECK-NEXT: STidx $a, 258, $x
...
---
name: push_a
# CHECK-LABEL: name: push_a
body: |
  bb.0.entry:
    liveins: $a
    Push $a
    RTS
# CHECK:      PHA
# CHECK-NEXT: RTS
...
---
name: push_x
# CHECK-LABEL: name: push_x
body: |
  bb.0.entry:
    liveins: $x
    Push $x
    RTS
# CHECK:      T_A $x
# CHECK-NEXT: PHA
# CHECK-NEXT: RTS
...
---
name: pull_a
# CHECK-LABEL: name: pull_a
body: |
  bb.0.entry:
    $a = Pull
    RTS implicit $a
# CHECK:      PLA
# CHECK-NEXT: RTS
...
---
name: pull_x
# CHECK-LABEL: name: pull_x
body: |
  bb.0.entry:
    $x = Pull
    RTS implicit $x
# CHECK:      PLA
# CHECK-NEXT: $x = TA_
# CHECK-NEXT: RTS
...
