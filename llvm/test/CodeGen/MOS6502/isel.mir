# RUN: llc -mtriple=mos6502 -run-pass=instruction-select -verify-machineinstrs -o - %s | FileCheck %s
---
name: asl
# CHECK-LABEL: name: asl
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
    %0:any(s8) = G_IMPLICIT_DEF
    %1:any(s8), %2:any(s1) = G_LSHO %0
    RTS implicit %1, implicit %2
# CHECK:      %0:azp = IMPLICIT_DEF
# CHECK-NEXT: %1:azp = ASL %0
# CHECK-NEXT: %2:anyi1 = COPY $c
# CHECK-NEXT: RTS
...
---
name: cmpimm_breq
# CHECK-LABEL: name: cmpimm_breq
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
    %0:any(s8) = G_IMPLICIT_DEF
    %1:any(s8) = G_CONSTANT i8 42
    %2:any(s1) = G_ICMP intpred(eq), %0, %1
    G_BRCOND %2, %bb.0
    RTS
# CHECK:      CMPimm %0, 42
# CHECK-NEXT: BR %bb.0, $z, 1
...
---
name: cmpimm_brne
# CHECK-LABEL: name: cmpimm_brne
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
    %0:any(s8) = G_IMPLICIT_DEF
    %1:any(s8) = G_CONSTANT i8 42
    %2:any(s1) = G_ICMP intpred(ne), %0, %1
    G_BRCOND %2, %bb.0
    RTS
# CHECK:      CMPimm %0, 42
# CHECK-NEXT: BR %bb.0, $z, 0
...
---
name: cmpimm_bruge
# CHECK-LABEL: name: cmpimm_bruge
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
    %0:any(s8) = G_IMPLICIT_DEF
    %1:any(s8) = G_CONSTANT i8 42
    %2:any(s1) = G_ICMP intpred(uge), %0, %1
    G_BRCOND %2, %bb.0
    RTS
# CHECK:      CMPimm %0, 42
# CHECK-NEXT: BR %bb.0, $n, 0
...
---
name: cmpimm_brult
# CHECK-LABEL: name: cmpimm_brult
legalized: true
regBankSelected: true
body: |
  bb.0.entry:
    %0:any(s8) = G_IMPLICIT_DEF
    %1:any(s8) = G_CONSTANT i8 42
    %2:any(s1) = G_ICMP intpred(ult), %0, %1
    G_BRCOND %2, %bb.0
