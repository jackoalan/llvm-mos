# RUN: llc -mtriple=mos6502 -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name:            test_tax
# CHECK-LABEL: name: test_tax
body:             |
  bb.0.entry:
    liveins: $a
    $x = COPY $a
# CHECK: $x = TA_
...
---
name:            test_txa
# CHECK-LABEL: name: test_txa
body:             |
  bb.0.entry:
    liveins: $x
    $a = COPY $x
# CHECK: T_A $x
...
---
name:            test_tay
# CHECK-LABEL: name: test_tay
body:             |
  bb.0.entry:
    liveins: $a
    $y = COPY $a
# CHECK: $y = TA_
...
---
name:            test_tya
# CHECK-LABEL: name: test_tya
body:             |
  bb.0.entry:
    liveins: $y
    $a = COPY $y
# CHECK: T_A $y
...
---
name:            test_txy
# CHECK-LABEL: name: test_txy
body:             |
  bb.0.entry:
    liveins: $x
    $y = COPY $x
# CHECK:      T_A $x
# CHECK-NEXT: $y = TA_
...
---
name:            test_tyx
# CHECK-LABEL: name: test_tyx
body:             |
  bb.0.entry:
    liveins: $y
    $x = COPY $y
# CHECK:      T_A $y
# CHECK-NEXT: $x = TA_
...
---
name:            test_preserve_nz
# CHECK-LABEL: name: test_preserve_nz
body:             |
  bb.0.entry:
    liveins: $a, $nz
    $x = COPY $a
    BNE %bb.0, implicit $nz
# CHECK:      PHP
# CHECK-NEXT: $x = TA_
# CHECK-NEXT: PLP
# CHECK-NEXT: BNE %bb.0, implicit $nz
...
---
name:            test_preserve_a
# CHECK-LABEL: name: test_preserve_a
body:             |
  bb.0.entry:
    liveins: $a, $x
    $y = COPY $x
    CMPimm $a, 0, implicit-def $nz
# CHECK:      PHA
# CHECK-NEXT: T_A $x
# CHECK-NEXT: $y = TA_
# CHECK-NEXT: PLA
...
---
name:            test_stzp
# CHECK-LABEL: name: test_stzp
body:             |
  bb.0.entry:
    liveins: $a
    $zp_0 = COPY $a
# CHECK: $zp_0 = STzpr $a
...
---
name:            test_ldzp
# CHECK-LABEL: name: test_ldzp
body:             |
  bb.0.entry:
    liveins: $zp_0
    $a = COPY $zp_0
# CHECK: $a = LDzpr $zp_0
...
