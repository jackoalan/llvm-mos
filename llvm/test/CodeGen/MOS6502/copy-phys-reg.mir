# RUN: llc -mtriple=mos6502 -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name:            tax
# CHECK-LABEL: name: tax
body:             |
  bb.0.entry:
    liveins: $a
    $x = COPY $a
# CHECK: $x = TA_
...
---
name:            txa
# CHECK-LABEL: name: txa
body:             |
  bb.0.entry:
    liveins: $x
    $a = COPY $x
# CHECK: T_A $x
...
---
name:            tay
# CHECK-LABEL: name: tay
body:             |
  bb.0.entry:
    liveins: $a
    $y = COPY $a
# CHECK: $y = TA_
...
---
name:            tya
# CHECK-LABEL: name: tya
body:             |
  bb.0.entry:
    liveins: $y
    $a = COPY $y
# CHECK: T_A $y
...
---
name:            txy
# CHECK-LABEL: name: txy
body:             |
  bb.0.entry:
    liveins: $x
    $y = COPY $x
# CHECK:      T_A $x
# CHECK-NEXT: $y = TA_
...
---
name:            tyx
# CHECK-LABEL: name: tyx
body:             |
  bb.0.entry:
    liveins: $y
    $x = COPY $y
# CHECK:      T_A $y
# CHECK-NEXT: $x = TA_
...
---
name:            preserve_nz
# CHECK-LABEL: name: preserve_nz
body:             |
  bb.0.entry:
    liveins: $a, $nz
    $x = COPY $a
    BR %bb.0, $z, 0
    RTS
# CHECK:      PHP implicit killed $p
# CHECK-NEXT: $x = TA_
# CHECK-NEXT: PLP
# CHECK-NEXT: BR %bb.0, $z, 0
...
---
name:            preserve_a
# CHECK-LABEL: name: preserve_a
body:             |
  bb.0.entry:
    liveins: $a, $x
    $y = COPY $x
    CMPimm $a, 0, implicit-def $nz
# CHECK:      PHA implicit killed $a
# CHECK-NEXT: T_A $x
# CHECK-NEXT: $y = TA_
# CHECK-NEXT: PLA
...
---
name:            stzp
# CHECK-LABEL: name: stzp
body:             |
  bb.0.entry:
    liveins: $a
    $zp_0 = COPY $a
# CHECK: $zp_0 = STzpr $a
...
---
name:            ldzp
# CHECK-LABEL: name: ldzp
body:             |
  bb.0.entry:
    liveins: $zp_0
    $a = COPY $zp_0
# CHECK: $a = LDzpr $zp_0
...
